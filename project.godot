; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Test"
run/main_scene="res://scenes/test_scene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

GlobalSignal="*res://global_signal.gd"
GlobalObjects="*res://global_objects.gd"

[display]

window/size/viewport_width=216
window/size/viewport_height=384
window/size/window_width_override=648
window/size/window_height_override=1152
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray()

[input]

move_left={
"deadzone": 0.5,
"events": []
}
move_right={
"deadzone": 0.5,
"events": []
}
move_up={
"deadzone": 0.5,
"events": []
}
move_down={
"deadzone": 0.5,
"events": []
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Environment"
2d_physics/layer_2="Player"
2d_physics/layer_3="Enemy"
2d_physics/layer_4="PlayerHurtbox"
2d_physics/layer_5="EnemyHurtbox"
2d_physics/layer_6="Buildings"
2d_physics/layer_7="PlayerBullets"
2d_physics/layer_8="EnemyBullets"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
